<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MeaData" namespace="MeaData" auto-import="false">
  <class name="Channel" table="channels" dynamic-update="true">
    <id name="Guid" column="Id">
      <generator class="guid"/>
    </id>
    <natural-id mutable="true">
      <many-to-one name="Recording" column="RecordingId" />
      <property name="Row" column="MeaRow" />
      <property name="Column" column="MeaColumn" />
    </natural-id>
    <set name="Cells" table="cells" lazy="extra" inverse="true" cascade="all">
      <key unique="true" column="ChannelId" />
      <one-to-many class="Cell"/>
    </set>
  </class>
</hibernate-mapping>
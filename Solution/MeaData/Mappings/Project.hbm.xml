<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MeaData" namespace="MeaData" auto-import="false">
  <class name="Project" table="projects" dynamic-update="true" dynamic-insert="true">
    <id name="Guid" column="Id">
      <generator class="guid"/>
    </id>
    <natural-id mutable="true">
      <property name="Name" column="Name" />
      <property name="DateStarted" column="DateStarted" />
    </natural-id>
    <property name="Comments" column="Comments" />
  </class>
</hibernate-mapping>

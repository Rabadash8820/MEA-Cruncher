<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MeaData" namespace="MeaData" auto-import="false">
  <class name="Recording" table="recordings" dynamic-update="true">
    <id name="Guid" column="Id">
      <generator class="guid"/>
    </id>
    <natural-id mutable="true">
      <many-to-one name="Tissue" column="TissueId" />
      <many-to-one name="Condition" column="ConditionId" />
      <property name="Number" column="RecordingNumber" />
    </natural-id>
    <property name="MeaRows" column="MeaRows"  />
    <property name="MeaColumns" column="MeaColumns" />
    <property name="Comments" column="Comments" />
    <set name="Channels" table="channels" lazy="extra" access="nosetter.camelcase-underscore" inverse="true" cascade="all">
      <key unique="true" column="RecordingId" />
      <one-to-many class="Channel"/>
    </set>
  </class>
</hibernate-mapping>